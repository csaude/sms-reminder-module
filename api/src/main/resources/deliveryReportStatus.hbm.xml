<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping
	package="org.openmrs.module.smsreminder.model">
	<class
		name="org.openmrs.module.smsreminder.model.DeliveryReportStatus"
		table="smsreminder_delivery_report_status" batch-size="25">
		<id name="id" type="int" column="delivery_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
		<property name="msgId" type="java.lang.Integer"
			column="msg_id" not-null="false" length="100" />
		<property name="deliveryReportDescription"
			type="java.lang.String" column="delivery_report_description"
			not-null="false" length="250" />
		<property name="deliveryReportReasonCode"
			type="java.lang.String" column="delivery_report_reason_code"
			length="15" />
		<property name="deliveryReportUpdateDatetime"
			type="java.util.Date" column="delivery_report_update_datetime" />
		<property name="deliveryReportStatus"
			type="java.lang.Integer" column="delivery_report_status"
			not-null="false" length="100" />
		<property name="messageSentDatetime" type="java.util.Date"
			column="message_sent_datetime" />
		<property name="nrSms" type="java.lang.Integer"
			column="nr_sms" not-null="false" length="150" />
		<property name="partnerMsgId" type="java.lang.String"
			column="partner_msg_id" not-null="false" length="150" />
		<property name="uuid" type="java.lang.String" column="uuid"
			not-null="false" length="150" />
		<property name="dateCreated" type="java.util.Date"
			column="date_created" not-null="true" length="100" />
	</class>
</hibernate-mapping>
