<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.smsreminder.model">
	<class name="org.openmrs.module.smsreminder.model.MessageSent" table="smsreminder_message_sent" batch-size="25">
		<id name="messageSentId" type="int" column="message_sent_id" unsaved-value="0" >
			<generator class="native" />
		</id>
		<discriminator column="message_sent_id" insert="false" />
		<property name="msgId" type="java.lang.Integer" column="msg_id" not-null="false" length="200000"/>
		<property name="phoneNumber" type="java.lang.String" column="phone_number" length="200" />
		<property name="nid" type="java.lang.String" column="nid" length="500" />
		<property name="fullName" type="java.lang.String" column="full_name" length="500" />
		<property name="gender" type="java.lang.String" column="gender" length="1" />
		<property name="lastVisitDate" type="java.util.Date" column="last_visit_date" />
		<property name="nextVisitDate" type="java.util.Date" column="next_visit_date" />
		<property name="alertDate" type="java.util.Date" column="alert_date" />
		<property name="dateCreated" type="java.util.Date" column="date_created" not-null="false" length="100" />
		<property name="reminderDays" type="java.lang.Integer" column="reminder_days" not-null="true" length="5"/>
		<property name="message" type="java.lang.String" column="message" not-null="false" length="1000"/>
		<property name="lastStatus" type="java.lang.String" column="last_status" not-null="false"  length="1000"/>
		<property name="lastDateStatus"  type="java.util.Date" column="last_date_status" not-null="false" />
		<many-to-one name="patient" class="org.openmrs.Patient" not-null="false" column="patient_id"/>
		<property name="PartnerMsgId" type="java.lang.String" column="partner_msg_Id" length="1000" />
	</class>
</hibernate-mapping>
