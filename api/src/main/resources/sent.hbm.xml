<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.smsreminder.model">
	<class name="org.openmrs.module.smsreminder.model.Sent" table="smsreminder_sent" batch-size="25">
		<id name="sentId" type="int" column="sent_id" unsaved-value="0" >
			<generator class="native" />
		</id>
		<discriminator column="sent_id" insert="false" />
		<property name="msgId" type="java.lang.Integer" column="msg_id" not-null="false" length="5"/>
		<property name="phoneNumber" type="java.lang.String" column="phone_number" length="15" />
		<property name="alertDate" type="java.util.Date" column="alert_date" />
		<property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="100" />
		<property name="reminderDays" type="java.lang.Integer" column="reminder_days" not-null="true" length="5"/>
		<property name="message" type="java.lang.String" column="message" not-null="true" length="150"/>
		<property name="status" type="java.lang.String" column="status" not-null="false"  length="150"/>
		<property name="statusDescriptionReason" type="java.lang.String" column="status_description_reason" not-null="true"  length="150"/>
		<property name="uuid" type="java.lang.String" column="uuid" not-null="false"  length="150"/>
		<many-to-one name="patient" class="org.openmrs.Patient" not-null="false" column="patient_id"/>
	</class>
</hibernate-mapping>
